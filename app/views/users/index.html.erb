 
<h1>User List</h1>
<div>  
  <div>   
    <%= link_to 'Add User', new_user_registration_path %>   
  </div>   
  <br>   

<table >
  <thead>
    <tr>  
        
        <th>User Email</th>
        <th>First Name </th>
        <th>Last Name </th>
        <th>Phone no.</th>
      
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email%></td>
        <td><%= user.first_name%></td>
        <td><%= user.last_name%></td>
        <td><%= user.phone_no%></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_registration_path(user) %></td>
        <td><%= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<%= will_paginate @user %>

