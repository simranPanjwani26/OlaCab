<p id="notice"><%= notice %></p> 
<h1 class="head">User List</h1>
<div class="row">  
  
<table >
  <thead>
    <tr>  
        
        <th>User Email</th>
        <th>First Name </th>
        <th>Last Name </th>
        <th>Phone no.</th>
      
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email%></td>
        <td><%= user.first_name%></td>
        <td><%= user.last_name%></td>
        <td><%= user.phone_no%></td>
        <td><%= link_to 'Show', user , class: 'btn btn-info'%></td>
        <td><%= link_to 'Edit', edit_user_registration_path(user), class: 'btn btn-warning' %></td>
        <td><%= link_to 'Delete', user_path(user), method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %></td>
          
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<%= will_paginate @user %>

